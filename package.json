{
  "name": "teleclinic",
  "version": "2.0.0",
  "description": "A telemedicine web application that provides seamless patient-to-provider communication",
  "keywords": [
    "firebase",
    "next",
    "react",
    "tailwindcss",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/JayBhatt2021/teleclinic.git"
  },
  "license": "MIT",
  "author": "Jay Bhatt (https://github.com/JayBhatt2021)",
  "type": "module",
  "scripts": {
    "build": "next build",
    "check-all": "npm run check-types && npm run check-format && npm run check-lint && npm run build",
    "check-format": "prettier . --check",
    "check-lint": "eslint .",
    "check-types": "tsc --noEmit --pretty",
    "dev": "next dev",
    "format": "prettier . --ignore-path .gitignore .prettierignore --write",
    "prepare": "husky",
    "start": "next start"
  },
  "dependencies": {
    "next": "14.2.13",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/node": "^22.7.4",
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "autoprefixer": "^10.4.20",
    "cssnano": "^7.0.6",
    "eslint": "^9.11.1",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.0",
    "firebase": "^10.13.2",
    "globals": "^15.9.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.5.4"
  }
}
